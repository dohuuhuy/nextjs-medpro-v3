{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-no-lambda": false,
    "curly": [true, "ignore-same-line"],
    "no-console": ["error", false],
    "no-namespace": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-var-requires": false
  }
}
