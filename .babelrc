{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@": "src/",
              "@config": "src/config/",
              "@components": "src/components",
              "@store": "src/store"
            }
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@src": "./src",
              "@config": "./src/config/",
              "@components": "./src/components",
              "@store": "./src/store"
            }
          }
        ]
      ]
    }
  }
}
