{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@config": "./src/config",
              "@components": "./src/components",
              "@componentStore": "./src/store/componentStore",
              "@utils": "./src/utils",
              "@store": "./src/store",
              "@templates": "./templates",
              "@assets": "./assets",
              "@public": "./public"
            }
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@config": "./src/config",
              "@components": "./src/components",
              "@componentStore": "./src/store/componentStore",
              "@utils": "./src/utils",
              "@store": "./src/store",
              "@templates": "./templates",
              "@assets": "./assets",
              "@public": "./public"
            }
          }
        ]
      ]
    }
  }
}
