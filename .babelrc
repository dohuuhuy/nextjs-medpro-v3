{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@components": "./src/components",
              "@containers": "./src/containers",
              "@common": "./src/common",
              "@store": "./src/store",
              "@moxy": "/node_modules/@moxy/",
              "@antd": "/node_modules/antd/",
              "@styles": "./public/assets/styles",
              "@template": "template",
              "@public": "public/",
              "@node_modules": ["node_modules/*"]
            }
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "@components": "./src/components",
              "@containers": "./src/containers",
              "@common": "./src/common",
              "@store": "./src/store",
              "@antd": "/node_modules/antd/",
              "@styles": "./public/assets/styles",
              "@template": "./src/template",
              "@medpro": ["/node_modules/@medpro/"]
            }
          }
        ]
      ]
    }
  }
}
